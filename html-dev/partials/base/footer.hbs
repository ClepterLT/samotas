        <footer id="footer" class="footer {{extraFooterClasses}}">
            <div class="row u-flex-center">
                
                    <div class="footer__column">
                        <h3 class="heading heading--tertiary">Kontaktai</h3>
                        <ul class="footer__list">
                            <li class="footer__item">{{siteTitle}}</li>
                            <li class="footer__item">{{footer.address}}</li>
                            {{#each footer.phones as |phone|}}
                                <li class="footer__item"><a href="tel: {{phone}}" class="footer__link">{{phone}}</a></li>
                            {{/each}}
                            <li class="footer__item"><a href="mailto: samotokeramika@gmail.com" class="footer__link">{{footer.email}}</a></li>
                        </ul>
                    </div>
                    <div class="footer__column">
                        <h3 class="heading heading--tertiary">Meniu</h3>
                        <ul class="footer__list">
                            {{#each header.links as |item|}}
                                <li class="footer__item"><a href="{{item.href}}" class="footer__link">{{item.title}}</a></li>
                            {{/each}}
                        </ul>
                    </div>
                    <div class="footer__column">
                        <h3 class="heading heading--tertiary">Galerija</h3>
                        <p class="footer__text">{{footer.gallery}}</p>
                    </div>
                    <div class="footer__column footer__social-column">
                        <div class="footer__social-wrap">
                            <h3 class="heading heading--tertiary">Bendraukime</h3>
                            <a href="{{socials.href}}" class="footer__social">
                                <svg version="1.1" xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 32 32">
                                    <title>{{socials.title}}</title>
                                    <path d="M29 0h-26c-1.65 0-3 1.35-3 3v26c0 1.65 1.35 3 3 3h13v-14h-4v-4h4v-2c0-3.306 2.694-6 6-6h4v4h-4c-1.1 0-2 0.9-2 2v2h6l-1 4h-5v14h9c1.65 0 3-1.35 3-3v-26c0-1.65-1.35-3-3-3z"></path>
                                </svg>
                            </a>
                        </div>
                        <p class="footer__copyright">{{{footer.copyright}}}</p>
                    </div>
                
            </div>
        </footer>  
        <script type="text/javascript" src="../assets/js/app.js"></script>
        <script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
    </body>
</html>